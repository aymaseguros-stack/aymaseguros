{
  "name": "aymaseguros-landing",
  "version": "2.0.0",
  "description": "Sistema completo de cotizaci√≥n de seguros con CRM integrado para Ayma Advisors",
  "type": "module",
  "author": "Ayma Advisors <contacto@aymaseguros.com.ar>",
  "license": "MIT",
  "homepage": "https://aymaseguros.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/aymaseguros-stack/aymaseguros.git"
  },
  "bugs": {
    "url": "https://github.com/aymaseguros-stack/aymaseguros/issues"
  },
  "keywords": [
    "seguros",
    "insurance",
    "cotizador",
    "crm",
    "react",
    "vite",
    "tailwindcss",
    "argentina",
    "rosario"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build && open dist/stats.html",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm test -- --run && npm run test:e2e",
    "playwright:install": "playwright install --with-deps",
    "serve": "vite preview --port 8080",
    "lint": "eslint src --ext .js,.jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\""
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "lucide-react": "^0.554.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^12.10.3",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "rollup-plugin-visualizer": "^5.11.0",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.1",
    "vite": "^5.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^1.0.4"
  }
}
